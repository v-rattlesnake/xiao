/* miniprogram/pages/login/login.wxss */
.content{
    width: 100%;
    height: 100vh;
    text-align: center;
    display: flex;
    justify-content: center;
    background: rgba(195, 176, 145);
}
.content text{
    width: 60rpx;
    display: block;
    font-size: 60rpx;
    margin: 0 20rpx;
    position: relative;
    top: 30%;
    color: #87CEFA;
}
.content text:first-of-type{
    top: 20%;
}
.content text:last-of-type{
    top: 40%;
}